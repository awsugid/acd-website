---
// Layout Imports
import MainLayout from '../layouts/MainLayout.astro';

// Full Astro Component Syntax:
// https://docs.astro.build/basics/astro-components/
---

<MainLayout>
	<main class="main">
    <!-- Hero Section -->
    <section id="hero" class="hero section dark-background">
      <div class="hero-background">
          <img src="assets/img/hero-bg.jpg" alt="Hero Background" class="hero-bg-img">
          <div class="overlay"></div>
      </div>
      <div class="hero-content">
          <img src="assets/img/acd.png" class="logo" title="AWS Community Day Indonesia">
          <h4>Uniting Developers and Community, Elevating Innovation with AWS</h4>
          <h5>Comming Soon</h5>
          <h6>TBA</h6>
          <a href="#about" class="btn-get-started">Start</a>
      </div>
    </section>


  </main>
</MainLayout>
